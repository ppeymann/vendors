{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "EditUserInput",
    "$protected": false,
    "type":"object",
    "properties": {
        "first_name":{
            "type": "string"
        },
        "last_name":{
            "type": "string"
        },
        "mobile":{
            "type": "string",
            "pattern": "(^[+][1-9]{1,4}[0-9]{2,3}[0-9]{4,7}$)|^$"
        }
    },
    "required": [
        "first_name",
        "last_name",
        "mobile"
    ]
}